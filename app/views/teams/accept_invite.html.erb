<p style="color: green"><%= notice %></p>

<%= render @team %>
<div class="my-2 p-2">
  <h2>Members</h2>
  <ul>
    <% @team.users.each do |member| %>
      <li><%= member.name %></li>
    <% end %>
  </ul>

<div>
  <%= link_to "Edit this team", edit_team_path(@team) %> |
  <%= link_to "Back to teams", teams_path %>

</div>
